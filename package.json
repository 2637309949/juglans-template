{
  "name": "web",
  "version": "1.0.0",
  "description": "Juglans Example",
  "main": "src/index.js",
  "bin": "src/index.js",
  "scripts": {
    "dev": "set taskEnv=local&&nodemon --inspect ./src/index.js",
    "apidoc": "apidoc -i ./src/ -o ./src/assets/public/apidoc",
    "binWn": "pkg package.json --target node10-win-x64 --out-path ./build/",
    "binLx": "pkg package.json --target node10-alpine-x64 --out-path ./build",
    "build": "gulp"
  },
  "keywords": [],
  "author": "Double",
  "license": "ISC",
  "pkg": {
    "scripts": [
      "./src/models/**/*.js",
      "./src/routes/**/*.js",
      "./src/services/**/*.js",
      "./src/tasks/**/*.js"
    ],
    "assets": [
      "./src/assets/public/**/*"
    ]
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "gulp-babel": "7.0.0",
    "gulp-sequence": "0.4.6",
    "gulp-uglify": "3.0.0"
  },
  "dependencies": {
    "fs-extra": "^7.0.0",
    "juglans": "git+https://github.com/2637309949/juglans.git",
    "koa-logger": "^3.2.0",
    "koa-static": "^5.0.0",
    "moment": "^2.22.2",
    "node-schedule": "^1.3.0"
  }
}
